{
    "name": "sav/opensearch",
    "description": "A laravel project ",
    "version": "1.0.0",
    "keywords": [
    ],
    "require": {
        "php": "^8.1",
        "laravel/lumen-framework": "^9.0",
        "opensearch-project/opensearch-php": "^2.2"
